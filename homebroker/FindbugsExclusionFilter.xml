<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>

     <!-- Ignorar todos os bugs no pacote homebroker.exemplos. -->
     <Match>
      <Class name="~homebroker.exemplos.*\.*" />
     </Match>
	 
	 <!-- Ignora o bug double check no método getInstance do motor do  -->
	 <!-- homebroker. -->
     <Match>
      <Class name="~homebroker.lógica_de_execução.MotorDoHomebroker" />
	  <Method name="getInstance" />
	  <Bug pattern="DC_DOUBLECHECK" params="" returns="MotorDoHomebroker" />
     </Match>

</FindBugsFilter>